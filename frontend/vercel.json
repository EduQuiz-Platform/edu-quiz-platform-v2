{"version": 2, "name": "edu-quiz-platform-v2", "framework": "vite", "installCommand": "cd frontend && npm install", "buildCommand": "cd frontend && npm run build", "outputDirectory": "frontend/dist", "env": {"VITE_SUPABASE_URL": "@supabase_url", "VITE_SUPABASE_ANON_KEY": "@supabase_anon_key", "VITE_GEMINI_AI_API_KEY": "@gemini_ai_api_key", "VITE_GOOGLE_AI_API_KEY": "@google_ai_api_key", "VITE_APP_NAME": "Educational Quiz Platform", "VITE_APP_VERSION": "1.0.0", "VITE_API_BASE_URL": "@api_base_url"}, "build": {"env": {"BUILD_MODE": "prod"}}, "headers": [{"source": "/(.*)", "headers": [{"key": "X-Content-Type-Options", "value": "nosniff"}, {"key": "X-Frame-Options", "value": "DENY"}, {"key": "X-XSS-Protection", "value": "1; mode=block"}]}, {"source": "/assets/(.*)", "headers": [{"key": "Cache-Control", "value": "public, max-age=31536000, immutable"}]}, {"source": "/api/(.*)", "headers": [{"key": "Cache-Control", "value": "public, max-age=0, must-revalidate"}]}], "rewrites": [{"source": "/((?!api/.*|assets/.*).*)", "destination": "/index.html"}], "redirects": [{"source": "/quiz", "destination": "/", "permanent": false}]}